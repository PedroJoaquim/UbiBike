{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "new trajectory",
  "description": "register bick pick drop",
  "type": "object",

  "properties": {

    "user_tid": {
      "description": "user trajectory id",
      "type": "string"
    },
    "start_sid":{
      "description": "start station id",
      "type": "integer"
    },
    "end_sid":{
      "description": "end station id",
      "type": "integer"
    },
    "coords": {
      "description": "the trajectory coords",
      "type": "array",
      "items":{
        "type":"object",
        "properties":{
          "lat":{"type":"number"},
          "lng":{"type":"number"}
        },
        "minItems": 1
      }
    },
    "start_timestamp":{
      "description": "ride start timestamp",
      "type": "integer"
    },
    "end_timestamp":{
      "description": "ride end timestamp",
      "type": "integer"
    },
    "distance":{
      "description": "ride distance in meters",
      "type": "number"
    }
  },
  "required": ["user_tid", "start_sid", "end_sid", "coords", "start_timestamp", "end_timestamp", "distance" ]
}
